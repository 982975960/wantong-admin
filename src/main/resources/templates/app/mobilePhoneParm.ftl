<@script src="/js/app/deviceManager.js"></@script>
<@script src="/js/3rd-party/lc/lc_switch.js"></@script>
<@link href="/css/app/mobileParm.css" rel="stylesheet"/>
<@link rel="stylesheet" href="/css/lc/lc_switch.css" />
<div class="main-w">
  <div class="content-wrap-w">
    <div class="content-r-path">应用管理 / 机型参数配置</div>
    <div class="content-box partner-detail-panel" id="mobileParmManager">
      <form id="searchFrom" action="" method="get">
        <input type="hidden" name="currentPage" id="currentPage" value="${page.currentPage!}"/>
        <input type="hidden" name="searchText" id="searchText" value="${searchText}">
        <input type="hidden" name="examine" id="examine" value="${examine}">
      </form>
      <div class="con-r-top">

        <div class="con-r-top-l" id="search">
          <select class="con-r-top-l-frame frame-line" name="examineStatusSelect" id="examineStatusSelect" style="width: 80px;margin-right:10px;">
            <option value="-1">全部</option>
            <option value="0">待审核</option>
            <option value="1">已审核</option>
          </select>
          <input type="text" style="width: 400px" id="searchInput" class="con-r-top-l-frame frame-line" value="${searchText}" placeholder="请输入机器型号开始搜索" />
          <button id="searchBookBtn" class="frame-Button">查找</button>
        </div>
        <div class="con-r-top-r" style="width: 42%;float: left;">
              <button class="frame-Button-b Button-left" id="defaultParamBtn" tname ="${defaultName}">传图全局控制设置</button>
        </div>
        <div class="con-r-top-r" style="width: 8%;float: left;">
          <@checkPrivilege url="/app/createDevice.do">
            <button class="frame-Button-b Button-left" id="addMobileParmBtn">配置机型参数</button>
          </@checkPrivilege>
        </div>
      </div>


      <div class="content-pro">
        <div class="text-block-con row-t">
          <ul>
            <li>
          <table width="100%" border="0" cellpadding="0" cellspacing="0">
            <thead>
            <tr class="text-block-head">
              <td width="20%" align="left" valign="middle" nowrap="nowrap" bgcolor="#f6f7fb" style="border-bottom:none;">序号</td>
              <td width="20%" align="left" valign="middle" nowrap="nowrap" bgcolor="#f6f7fb" style="border-bottom:none;">机器型号</td>
              <td width="20%" align="left" valign="middle" nowrap="nowrap" bgcolor="#f6f7fb" style="border-bottom:none;">机型别名</td>
              <td width="20%" align="left" valign="middle" nowrap="nowrap" bgcolor="#f6f7fb" style="border-bottom:none;">引用机型</td>
              <td width="20%" align="left" valign="middle" nowrap="nowrap" bgcolor="#f6f7fb" style="border-bottom:none;">操作</td>
            </tr>
            </thead>
            <tbody>
            <#list page.data as l>
              <tr>
                <td align="left">
                  ${l_index+1}
                </td>
                <td align="left">
                  ${l.model}
                </td>
                <td align="left">
                  ${l.modelOtherName}
                </td>
                <td align="left">
                  ${l.ref_machineModel}
                </td>
                <td align="left">
                  <@checkPrivilege url="/app/checkDeviceName.do">
                    <button class="Button-line btn-look btn-info" id="lookMobileParmBtn" tid="${l.id}"
                            tname="${l.model}">
                      查看
                    </button>
                  </@checkPrivilege>
                    <button class="Button-line btn-edit btn-info" id="editMobileParmBtn" tid="${l.id}"
                            tname="${l.model}">
                      编辑
                    </button>
                </td>
              </tr>
            </#list>
            </tbody>
          </table>

            </li>
          </ul>
        </div>
      </div>

      <!--分页-->
      <#if (page.totalPage>1)>
        <div id="pageline" class="con-page" style="border:none; margin-top:1%;">
          <ul>
            <#--<li><a href="#" page="${page.first}">首页</a></li>-->
            <li class="page-back"><a href="#" page="${page.prev}"><img src="/static/images/ico9_03.png"></a></li>
            <#list page.arrPage as p>
              <#if p==page.currentPage>
                <li class="page-back-b"><a page="0">${p}</a></li>
              <#elseif p==-1>
                <li class="disabled page-back-b2"><a href="#" onclick="return false;">...</a></li>
              <#else >
                <li class="page-back-b2"><a href="#" page="${p}">${p}</a></li>
              </#if>
            </#list>
            <li class="page-back"><a href="#" page="${page.next}"><img src="/static/images/ico9_05.png"></a></li>
            <Li>到第</Li>
            <Li><input name="text" type="text" class="page-box page-back" id="pageText"/></Li>
            <Li>页</Li>
            <Li><input name="" type="submit" value="跳转" class="page-input" id="pageJump"/></Li>
          </ul>
        </div>
      </#if>
      <!--分页-->

      <!-- 创建新机型 -->
      <div class="row create-partner-container" id="createMobileParmContainer" style="display:none">
        <div class="col-md-12">
          <form id="createMobileParmForm" method="post" action="">

            <div class="layui-tab layui-tab-brief" lay-filter="docDemoTabBrief" style="margin-top: 0">
              <ul class="layui-tab-title">
                <li class="layui-this" id="paramConfigTab">参数配置</li>
                <li id="uploadTimeTab">传图频次设置</li>
              </ul>
            </div>

            <div id = "paramConfigDiv" >
              <div class="form-group">
                <label for="pageType">机器型号：</label>
                <input id="name" type="text" class="form-control">
                <input type="checkbox" id="baseMobile" style="margin-left: 50px">
                <label>基础机型</label>
              </div>
              <div class="form-group">
                <label for="pageType">机型别名：</label>
                <input id="otherName" type="text" class="form-control">
              </div>
              <div class="form-group">
                <label>引用机型：</label>
                <!--引用机型-->
                <select id="ref_machine" class="con-r-top-l-frame frame-line" style="width: 60%;max-width: none">
                  <option value="0">（无）</option>
                    <#list devicelist as model >
                      <option value="${model.id}">${model.model}</option>
                    </#list>
                </select>
              </div>
              <div  class="form-group">
                <label>预处理：</label>
                <div class="flip-select">
                  <input type="checkbox" id="flip"/>
                  <label>翻转</label>
                  <select id="flipSelect" class="con-r-top-l-frame frame-line">
                    <option value="0">（无）</option>
                    <option value="-1">水平垂直翻转</option>
                    <option value="1">上下翻转</option>
                    <option value="2">左右翻转</option>
                  </select>
                </div>
                <div class="flip-select" style="margin-left: 6%">
                  <input type="checkbox" id="rotate"/>
                  <label>旋转</label>
                  <select id="rotateSelect" class="con-r-top-l-frame frame-line">
                    <option value="0">（无）</option>
                    <option value="90">旋转90°</option>
                    <option value="180">旋转180°</option>
                    <option value="270">旋转270°</option>
                  </select>
                </div>
              </div>

              <div class="form-group" style="width: 50%;float: left">
                <label style="width: 20%">实拍图:</label>
                <div id="uploadRealBtn" style="display: inline">上传</div>
                <label style="width: auto;margin-left: 20px">jpg/jpeg格式</label>
                <img id="realImage"
                     src="data:image/jpeg;base64,"
                     style="margin-left: 20%;margin-top: 10px;width: 215px;height: 154px;" alt="上传实拍图" class="img-thumbnail">
              </div>

              <div class="form-group" style="width: 50%;float: left">
                <label style="width: 20%">模版图:</label>
                <div id="uploadStencilrBtn" style="display: inline">上传</div>
                <label style="width: auto;margin-left: 20px">jpg/jpeg格式</label>
                <img id="stencilImage"
                     src="data:image/jpeg;base64,"
                     style="margin-left: 20%;margin-top: 10px;width: 215px;height: 154px;" alt="上传模版图" class="img-thumbnail">
              </div>

              <div class="form-group" id="parm-div">
                <label style="float: left">高级参数:</label>
                <div style="height: 175px;width: 90%;float: left" id="highParam">
                  <div class="parm-div">
                    <label>云参数</label>
                    <textarea id="cloudParm" value="" class="form-control"></textarea>
                  </div>
                  <div class="parm-div" style="margin-left: 15%">
                    <label>端参数</label>
                    <textarea id="edgeParm" value="" class="form-control"></textarea>
                  </div>
                </div>
              </div>

              <div class="form-group">
                <label style="float: left">工程参数:</label>
                <textarea id="proParam" value="" class="form-control" style="width: 560px;height: 60px;resize: none"></textarea>
              </div>

              <div class="form-group">
                <label style="float: left">端算法参数:</label>
                <textarea id="edgeAljParam" value="" class="form-control" style="width: 560px;height: 60px;resize: none"></textarea>
              </div>

              <div class="form-group">
                <label style="float: left">定位算法参数:</label>
                <textarea id="fixAljParam" value="" class="form-control" style="width: 560px;height: 60px;resize: none"></textarea>
              </div>

              <div class="form-group">
                <label style="float: left">备注:</label>
                <textarea id="note" value="" class="form-control" style="width: 560px;height: 60px;resize: none"></textarea>
              </div>
            </div>

            <div id="uploadTimeDiv" style="display: none;">

              <div id="frontGapDiv" style="height: 90px;" class="parent-div">

                <div class="request-set">
                  <label for="pageType">应用启动传图请求设置</label>
                  <input type="checkbox" name="check-1" value="1" class="lcs_check" autocomplete="off" />
                </div>

                <div class="rate-set">
                  <label for="pageType" class="rate-label">最高传图频率:</label>
                  <input id="rate" type="text"  disabled="disabled" onkeyup="this.value=this.value.replace(/[^0-9]+/,'');">
                  <label for="pageType" class="rate-ms">ms</label>
                </div>

              </div>

              <div id="uploadGapDiv" style="height: 90px;" class="parent-div">

                <div class="request-set">
                  <label for="pageType">翻页传图请求设置</label>
                  <input type="checkbox" name="check-1" value="1" class="lcs_check" autocomplete="off" />
                </div>

                <div class="rate-set">
                  <label for="pageType"  class="rate-label">最高传图频率:</label>
                  <input id="rate" type="text" disabled="disabled" onkeyup="this.value=this.value.replace(/[^0-9]+/,'');">
                  <label for="pageType" class="rate-ms">ms</label>
                </div>

              </div>

              <div id="coverGapDiv" style="height: 90px;" class="parent-div">

                <div class="request-set">
                  <label for="pageType">换书传图请求设置</label>
                  <input type="checkbox" name="check-1" value="1" class="lcs_check" autocomplete="off" />
                </div>

                <div class="rate-set">
                  <label for="pageType"  class="rate-label">最高传图频率:</label>
                  <input id="rate" type="text" disabled="disabled" onkeyup="this.value=this.value.replace(/[^0-9]+/,'');">
                  <label for="pageType" class="rate-ms">ms</label>
                </div>

              </div>

              <div id="fingerDelayDiv" style="height: 90px;" class="parent-div">

                <div class="request-set">
                  <label for="pageType">手指传图请求设置</label>
                  <input type="checkbox" name="check-1" value="1" class="lcs_check" autocomplete="off" />
                </div>

                <div class="rate-set">
                  <label for="pageType"  class="rate-label">最高传图频率:</label>
                  <input id="rate" type="text" disabled="disabled" onkeyup="this.value=this.value.replace(/[^0-9]+/,'');">
                  <label for="pageType" class="rate-ms">帧</label>
                </div>

              </div>

              <div id="handleEdgeDiv" style="height: 50px;" class="parent-div">

                <div class="request-set">
                  <label for="pageType">A3视野下图片底部特殊处理</label>
                  <input type="checkbox" name="check-1" value="1" class="lcs_check" autocomplete="off" />
                </div>

              </div>

              <div id="proposeHandDiv" style="height: 50px;" class="parent-div">
                <div class="request-set">
                  <label for="pageType">寻找手指区域</label>
                  <input type="checkbox" name="check-1" value="1" class="lcs_check" autocomplete="off" />
                </div>
              </div>

              <div id="defaultTipDiv" style="display: none;">
                <span >注:<p></span>
                <span >1.当以上某项关闭后，使用关闭后的默认值，在指定机型中相应的设置均不生效！<p></span>
                <span for="pageType">2.当以上某项开启后，如果在指定机型中也设置了相应项，则已机型设置为主;如指
                  定机型未设置相应项，则已全局控制设置项的值为主</span>
              </div>

            </div>

            <div class="modal-footer" style="width: 100%;float: left;">
              <@checkPrivilege url="/app/changeDeviceExamine.do">
              <button type="button" style="display: none;margin-right: 50%;margin: 0;float: left;" id="saveAndExamineBtn" class="pop-padding frame-Button-b">保存并审核通过</button>
              </@checkPrivilege>
              <button type="button" id="saveBtn" class="pop-padding frame-Button-b">保存</button>
              <button type="button" id="closeBtn" class="pop-padding frame-Button">关闭</button>
            </div>

          </form>
        </div>
      </div>
      <!-- 创建供应商 -->

      <!-- 配置查重 -->
      <div class="create-partner-container" id="deviceCheckContainer" style="display:none;width: 90%">
        <div class="col-md-12">
          <form id="deviceCheckForm" method="post" action="" onsubmit="return false;">
            <div class="form-group" >
              <label for="pageType" class="input-group-addon">机器型号：</label>
              <input id="name" type="text" class="frame-line con-r-top-l-frame" style="width: 50%;float: left">
              <@checkPrivilege url="/app/checkDeviceName.do">
                <div id="checkBtn">
                  <button type="button" id="deviceCheckBtn" class="btn btn-default">配置查重</button>
                </div>
              </@checkPrivilege>
            </div>

          </form>
        </div>
      </div>
      <!-- 配置查重 -->
    </div>
  </div>
</div>
<script>
  $(function () {
    wantong.deviceManager.init();
  });
</script>